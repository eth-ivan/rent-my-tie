<%= simple_form_for :search, method: "get" do |f| %>
  <h2>Booking a date</h2>
  <br>
  <div class="row">
    <div class="col-md-4">
      <%= f.input :start_date, as: :string, required: false, label: false, placeholder: "Start date", input_html: {class: "datepicker"} %>
    </div>
    <div class="col-md-4">
      <%= f.input :start_date, as: :string, required: false, label: false, placeholder: "End date", input_html: {class: "datepicker"} %>
    </div>
    <div class="ml-5">
      <%= f.button :submit, "Book now", class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
